#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n,x,y;
	int t;
	cin>>n;
	int a[n];
	int count[n];
	for(int i=1;i<=n;i++)
	{
		cin>>a[i];
	}
	for(int i=1;i<=n;i++)
	{
		count[i]=a[i];
	}
	cin>>t;
	
	while(t--)
	{
		cin>>x>>y;
		if(x>=2 && x<n)
		{
			count[x-1]+=y-1;
		count[x+1]+=count[x]-y;
		count[x]=0;
			
		}
		else if(x==1) 
	{
		
			
		count[x+1]+=count[x]-y;
		count[x]=0;
	}
	else if(x==n)
	{
		
		count[x-1]+=y-1;
		count[x]=0;
		
	}
		
		
	}
	for(int i=1;i<=n;i++)
	{
		cout<<count[i]<<endl;
	}
	return 0;
}
