#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int arr[4];
	char str[1000];
	int c1,c2,c3,c4;
	c1=c2=c3=c4=0;
	int ans=0;
	for(int i=0;i<4;i++)
	{
		cin>>arr[i];
	}
	cin>>str;
	//cout<<str<<endl;
	
	for(int i=0;str[i]!='\0';i++)
	{
		if(str[i]=='1')
		c1++;
		else if(str[i]=='2')
		c2++;
	    else if(str[i]=='3')
		c3++;
		else if(str[i]=='4')
		c4++;
	}
	//cout<<c1<<c2<<c3<<c4<<endl;
	
		ans+=(c1*arr[0]+c2*arr[1]+c3*arr[2]+c4*arr[3]);
	
	cout<<ans;
	
	
	return 0;
}
