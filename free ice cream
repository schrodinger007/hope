#include <bits/stdc++.h>
using namespace std;
#define ll long long int
 
int main() {
	// your code goes here
	ll n,x,d;
	char ch;
	cin>>n>>x;
	ll count=0;
	while(n--)
	{
		cin>>ch>>d;
		if(ch=='+')
		{
			x=x+d;
		}
		if(ch=='-')
		{
			if(x>=d)
			{
				x=x-d;
			}
			else
			{
				count++;
			}
		}
	}
	cout<<x<<' '<<count;
	
	return 0;
}
