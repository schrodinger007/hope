#include <bits/stdc++.h>
using namespace std;
#define ll long long int
int main() {
	// your code goes here
	ll n,b,d;

	ll count=0;
	cin>>n>>b>>d;
	ll a[n];
		ll sum[n];
	for(ll i=0;i<n;i++)
	{
		cin>>a[i];
		
		
	}
	for(ll i=0;i<n;i++)
	{
		if(a[i]>b)
		{
			a[i]=0;
		}
		//sum[i]=0;
	
	}
	for(ll i=0;i<n;i++)
	{
		sum[i]=0;
	}
	
	sum[0]=a[0];
	for(ll i=1;i<n;i++)
	{
			sum[i]=a[i]+sum[i-1];
			if(sum[i]>d)
			{
				sum[i]=0;
				count++;
			}
	}
	
	cout<<count;
	return 0;
}
