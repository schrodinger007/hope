#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int a[n+1];
	int b[n+1];
	int count=0;
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	for(int i=0;i<=n;i++)
 {
 for(int j=0;j<=n-i;j++)
 {
 if(a[j]>a[j+1])
 {
 int temp=a[j];
 a[j]=a[j+1];
 a[j+1]=temp;
 }
 }
 }
 
for(int i=0;i<n;i++)
{
	if(a[i]<0)
	{
		count++;
		b[i]=a[i];
	}
}
int desc;
 for (int i = 0; i < count; i++)   //  'for' loop is used for sorting the numbers in descending order
    {
        for (int j = i + 1; j < count; j++)
        {
            if (b[i] < b[j])
            {
                desc = b[i];
                b[i] = b[j];
                b[j] = desc;
            }
        }
    }
    for(int i=0;i<count;i++)
    
    {
    	a[i]=b[i];
    }
    for(int i=count;i<n;i++)
    {
    	a[i]=a[i];
    }
    for(int i=0;i<n;i++)
    {
    	cout<<a[i]<<'\t';
    }
 
	return 0;
}
