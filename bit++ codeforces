#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int n;
	char str[10000];
	int x=0;
	cin>>n;
	while(n--)
	{
		cin>>str;
		for(int i=0;str[i]!='\0';i++)
	{
		if(str[i]=='+'&&str[i+1]=='+')
		{
			x=x+1;
		}
		else if(str[i]=='-'&&str[i+1]=='-')
		{
			x=x-1;
		}
	}	
	}
	cout<<x;
 
	return 0;
}
