#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	char str[10000];
	cin>>str;
	int k=0;
	char str2[10000];
	for(int i=0;str[i]!='\0';i++)
	{
		if(!(str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U'||str[i]=='Y'||str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='y'))
		{
			if(isupper(str[i]))
			{
				str[i]=tolower(str[i]);
			}
		  	str2[k]='.';
		  	str2[k+1]=str[i];
		  	k=k+2;
		}
	}
	cout<<str2;
	return 0;
}
